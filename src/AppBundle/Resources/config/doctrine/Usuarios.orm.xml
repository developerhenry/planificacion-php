<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuarios" table="usuarios">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="apenom" type="string" column="apenom" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomusuario" type="string" column="nomusuario" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clave" type="string" column="clave" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isActive" type="integer" column="is_active" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
